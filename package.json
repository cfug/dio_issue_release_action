{
  "name": "dio_issue_release_action",
  "version": "0.1.0",
  "private": true,
  "description": "Add github release when issue closed.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "pkg-main": "ncc build --source-map --license licenses.txt",
    "pkg-install": "ncc build lib/check-need-install.js -o dist/install",
    "test": "jest",
    "dist": "pnpm run build && pnpm run pkg-main && pnpm run pkg-install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfug/dio_issue_release_action"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "release",
    "dart",
    "pub"
  ],
  "author": "CaiJingLong",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/core": "^4.2.0",
    "@octokit/rest": "^19.0.8",
    "semver": "^7.5.1",
    "shelljs": "^0.8.5",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/node": "^18.16.12",
    "@types/semver": "^7.5.0",
    "@types/shelljs": "^0.8.12",
    "@typescript-eslint/parser": "^5.59.6",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.40.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}