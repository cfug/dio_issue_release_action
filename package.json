{
  "name": "dio_issue_release_action",
  "version": "0.1.0",
  "private": true,
  "description": "Add github release when issue closed.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "dist": "pnpm run build && pnpm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfug/dio_issue_release_action"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "release",
    "dart",
    "pub"
  ],
  "author": "CaiJingLong",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/core": "^4.2.0",
    "@octokit/rest": "^19.0.7",
    "semver": "^7.3.8",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/node": "^18.14.1",
    "@types/semver": "^7.3.13",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/parser": "^5.53.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.34.0",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}