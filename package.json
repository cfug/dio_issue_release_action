{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "dist": "pnpm run build && pnpm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/core": "^4.2.0",
    "@octokit/rest": "^19.0.7",
    "semver": "^7.3.8",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/node": "^18.14.0",
    "@types/semver": "^7.3.13",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.6.1",
    "eslint-plugin-jest": "^25.7.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5"
  }
}